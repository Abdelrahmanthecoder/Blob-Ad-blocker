{
  "manifest_version": 3,
  "name": "BLOB Adblocker",
  "version": "1.1.0",
  "description": "Welcome to BLOB Adblocker, your one-stop shop for ad blocking!",
  "permissions": [
    "scripting",
    "webRequest",
    "webRequestBlocking",
    "hostPermissions"
  ],
